{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++14 *.cpp  -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    },
    "files.associations": {
        "iostream": "cpp",
        "cmath": "cpp",
        "iomanip": "cpp",
        "ostream": "cpp",
        "new": "cpp",
        "typeinfo": "cpp",
        "*.tcc": "cpp",
        "limits": "cpp",
        "type_traits": "cpp"
    }
}